{
    "name": "ogrre/laravel-media",
    "description": "Package to manage media easy in laravel",
    "type": "library",
    "require": {
        "php" : "^7.3|^8.0|^8.1",
        "illuminate/database": "^7.0|^8.0|^9.0|^10.0",
        "illuminate/support": "^7.0|^8.0|^9.0|^10.0"
    },
    "homepage": "https://github.com/ogrre/laravel-media",
    "license": "MIT",
    "authors": [
        {
            "name": "Ogrre",
            "homepage": "https://jbloup.dev",
            "role": "Developer"
        }
    ],
    "autoload": {
        "psr-4": {
            "Ogrre\\Media\\": "src/"
        }
    },
    "config": {
        "sort-packages": true
    },
        "extra": {
        "laravel": {
            "providers": [
                "Ogrre\\Media\\MediaServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
